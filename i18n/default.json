		    "npc.Malaya.h8_10.green_rain.1": "This world is full of surprises.",
		    "npc.Malaya.h8_10.green_rain.2": "Stay close, {{Player}}.",

        //character: Mr. Aguar
			"npc.MrAguar.h0_3.morning.1": "Do not disturb me. I am collecting samples.",
			"npc.MrAguar.h0_3.morning.2": "The pH levels are fluctuating today.",
			"npc.MrAguar.h0_3.morning.3": "Why are you here? I am busy.",
			"npc.MrAguar.h0_3.morning.4": "Farming run-off is a serious concern.",
			"npc.MrAguar.h0_3.afternoon.1": "I have no time for small talk.",
			"npc.MrAguar.h0_3.afternoon.2": "Observe the water clarity. It is adequate.",
			"npc.MrAguar.h0_3.afternoon.3": "Don't touch my equipment.",
			"npc.MrAguar.h0_3.afternoon.4": "The ecosystem is fragile. Do not break it.",
